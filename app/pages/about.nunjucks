<!-- index.nunjucks -->
{% extends "layout.nunjucks" %}


{% block content %} 

<h1>This is the index page</h1>
{% import 'macros/navigation.nunjucks' as nav %}
{{nav.active('about')}}

<div class="gallery">
  {% for image in images %}
  <div class="gallery__item">
    <img src="{{image.src}}" alt="{{image.alt}}">
  </div>
  {% endfor %}
</div>

{% endblock %}