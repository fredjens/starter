<!-- index.nunjucks -->
{% extends "layout.nunjucks" %}


{% block content %} 
{% import 'macros/navigation.nunjucks' as nav %}
{{nav.active('home')}}

<h1>Find what you love and let it kill you</h1>

<div class="gallery">
  {% for image in images %}
  <div class="gallery__item">
    <img src="{{image.src}}" alt="{{image.alt}}">
  </div>
  {% endfor %}
</div>

{% endblock %}